{
  "name": "lint-deps",
  "description": "CLI tool that tells you when dependencies are missing from package.json and offers you a choice to install them. Also tells you when dependencies are listed in package.json but are not being used anywhere in your project. Node.js command line tool and API.",
  "version": "3.0.2",
  "homepage": "https://github.com/lint-deps/lint-deps",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "contributors": [
    "Brian Woodward (https://twitter.com/doowb)",
    "hemanth.hm (http://h3manth.com)",
    "Jake Verbaten (http://github.com/Raynos)",
    "Jon Schlinkert (http://twitter.com/jonschlinkert)",
    "Mark Dalgleish (markdalgleish.com)"
  ],
  "repository": "lint-deps/lint-deps",
  "bugs": {
    "url": "https://github.com/lint-deps/lint-deps/issues"
  },
  "license": "MIT",
  "files": [
    "bin",
    "index.js",
    "lib"
  ],
  "main": "index.js",
  "bin": {
    "deps": "bin/cli.js"
  },
  "engines": {
    "node": ">=8.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "ansi-colors": "^3.2.1",
    "arr-diff": "^4.0.0",
    "arr-union": "^3.1.0",
    "array-unique": "^0.3.2",
    "arrayify-compact": "^0.2.0",
    "base": "^3.0.0",
    "base-cwd": "^0.3.4",
    "base-npm": "^0.4.1",
    "base-option": "^1.0.0",
    "base-pkg": "^1.0.0",
    "base-task": "^2.0.1",
    "base-yarn": "^1.0.0",
    "clone-deep": "^4.0.0",
    "data-store": "^3.1.0",
    "enquirer": "^2.0.7",
    "find-pkg": "^2.0.0",
    "get-value": "^3.0.1",
    "global-modules": "^1.0.0",
    "inflection": "^1.12.0",
    "is-glob": "^4.0.0",
    "is-valid-glob": "^1.0.0",
    "kind-of": "^6.0.2",
    "log-utils": "^1.0.0",
    "match-requires": "^2.0.1",
    "matched": "^3.0.1",
    "merge-configs": "^3.0.0",
    "merge-deep": "^3.0.2",
    "micromatch": "^3.1.10",
    "object.omit": "^3.0.0",
    "object.pick": "^1.3.0",
    "omit-empty": "^0.4.1",
    "path-starts-with": "^1.0.0",
    "set-value": "^3.0.0",
    "strip-comments": "^1.0.2",
    "text-table": "^0.2.0",
    "union-value": "^1.0.0",
    "validate-npm-package-name": "^3.0.0",
    "vinyl": "^2.2.0",
    "word-wrap": "^1.2.3",
    "write": "^1.0.3",
    "write-json": "^3.0.1",
    "yargs": "^12.0.4"
  },
  "devDependencies": {
    "mocha": "^5.2.0",
    "gulp": "^3.9.1",
    "gulp-mocha": "^6.0.0",
    "gulp-istanbul": "^1.1.3",
    "gulp-eslint": "^5.0.0",
    "gulp-unused": "^0.2.1",
    "gulp-format-md": "^2.0.0"
  },
  "keywords": [
    "dep",
    "dependencies",
    "dependency",
    "deps",
    "detect",
    "detective",
    "devDependencies",
    "extra",
    "find",
    "installed",
    "lint",
    "missing",
    "new",
    "require",
    "required",
    "requires",
    "unused"
  ],
  "eslintConfig": {
    "rules": {
      "no-console": 0
    }
  },
  "verb": {
    "toc": "collapsible",
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    }
  }
}