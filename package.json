{
  "name": "lint-deps",
  "description": "Lint deps",
  "version": "0.1.0",
  "homepage": "https://github.com/jonschlinkert/lint-deps",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/lint-deps",
  "bugs": {
    "url": "https://github.com/jonschlinkert/lint-deps/issues"
  },
  "license": "MIT",
  "files": [
    "bin",
    "index.js",
    "lib"
  ],
  "main": "index.js",
  "bin": {
    "deps": "bin/cli.js"
  },
  "engines": {
    "node": ">=4.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "arr-diff": "^3.0.0",
    "arr-union": "^3.1.0",
    "arrayify-compact": "^0.2.0",
    "async-each": "^1.0.1",
    "base-app": "^0.2.6",
    "base-ignore": "^0.3.0",
    "base-npm": "^0.4.1",
    "base-option": "^0.8.4",
    "base-pkg": "^0.2.4",
    "base-questions": "^0.9.1",
    "clone-deep": "^0.2.4",
    "data-store": "^0.16.1",
    "delete": "^0.3.2",
    "enquirer": "^0.4.1",
    "enquirer-prompts": "^0.1.1",
    "export-files": "^2.1.1",
    "extend-shallow": "^2.0.1",
    "fs-exists-sync": "^0.1.0",
    "get-value": "^2.0.6",
    "kind-of": "^3.1.0",
    "lazy-cache": "^2.0.2",
    "log-ok": "^0.1.1",
    "log-utils": "^0.2.1",
    "match-requires": "^1.0.2",
    "merge-deep": "^3.0.0",
    "micromatch": "^2.3.11",
    "mixin-deep": "^1.1.3",
    "object.omit": "^2.0.1",
    "prompt-checkbox": "^0.4.2",
    "strip-comments": "^0.4.4",
    "through2": "^2.0.3",
    "union-value": "^0.2.3",
    "word-wrap": "^1.1.0",
    "yargs-parser": "^4.0.2"
  },
  "devDependencies": {
    "global-modules": "^0.2.3",
    "gulp-format-md": "^0.1.11",
    "homedir-polyfill": "^1.0.1",
    "matched": "^0.4.4",
    "mocha": "^3.2.0",
    "read-data": "^0.3.0"
  },
  "keywords": [
    "dependencies",
    "deps",
    "devDependencies",
    "extra",
    "lint",
    "missing",
    "new",
    "requires",
    "unused"
  ],
  "verb": {
    "toc": "collapsible",
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    },
    "related": {
      "list": []
    },
    "reflinks": [
      "verb",
      "verb-generate-readme"
    ]
  }
}
