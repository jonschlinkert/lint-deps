{
  "name": "lint-deps",
  "description": "CLI tool that tells you when dependencies are missing from package.json and offers you a choice to install them. Also tells you when dependencies are listed in package.json but are not being used anywhere in your project. Node.js command line tool and API.",
  "version": "1.1.4",
  "main": "index.js",
  "homepage": "https://github.com/jonschlinkert/lint-deps",
  "author": {
    "name": "Jon Schlinkert",
    "url": "https://github.com/jonschlinkert"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jonschlinkert/lint-deps.git"
  },
  "bugs": {
    "url": "https://github.com/jonschlinkert/lint-deps/issues"
  },
  "license": {
    "type": "MIT",
    "url": "https://github.com/jonschlinkert/lint-deps/blob/master/LICENSE"
  },
  "preferGlobal": true,
  "bin": {
    "deps": "./cli.js"
  },
  "dependencies": {
    "arr-flatten": "^1.0.0",
    "array-unique": "^0.1.1",
    "chalk": "^0.5.1",
    "commandments": "^0.2.2",
    "debug": "^2.1.1",
    "filter-object": "^1.1.0",
    "globby": "^1.1.0",
    "inquirer": "^0.8.0",
    "load-pkg": "^1.0.0",
    "lodash": "^2.4.1",
    "log-symbols": "^1.0.1",
    "match-requires": "^0.3.0",
    "micromatch": "^1.0.1",
    "minimist": "^1.1.0",
    "spawn-commands": "^0.1.0",
    "verbalize": "^0.1.2",
    "word-wrap": "^1.0.0",
    "write-json": "^0.1.0"
  },
  "devDependencies": {
    "helper-reflinks": "^0.4.0"
  },
  "keywords": [
    "dep",
    "dependencies",
    "dependency",
    "deps",
    "detect",
    "detective",
    "find",
    "installed",
    "lint",
    "missing",
    "require",
    "required",
    "requires"
  ]
}
