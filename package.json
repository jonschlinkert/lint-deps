{
  "name": "lint-deps",
  "description": "CLI tool that tells you when dependencies are missing from package.json and offers you a choice to install them. Also tells you when dependencies are listed in package.json but are not being used anywhere in your project. Node.js command line tool and API.",
  "version": "3.0.2",
  "homepage": "https://github.com/lint-deps/lint-deps",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "contributors": [
    "Brian Woodward (https://twitter.com/doowb)",
    "hemanth.hm (http://h3manth.com)",
    "Jake Verbaten (http://github.com/Raynos)",
    "Jon Schlinkert (http://twitter.com/jonschlinkert)",
    "Mark Dalgleish (markdalgleish.com)"
  ],
  "repository": "lint-deps/lint-deps",
  "bugs": {
    "url": "https://github.com/lint-deps/lint-deps/issues"
  },
  "license": "MIT",
  "files": [
    "bin",
    "index.js",
    "lib"
  ],
  "main": "index.js",
  "bin": {
    "deps": "bin/cli.js"
  },
  "engines": {
    "node": ">=8.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "@folder/xdg": "^2.1.0",
    "ansi-colors": "^3.2.4",
    "base": "^3.0.0",
    "base-cwd": "^0.3.4",
    "base-npm": "^0.4.1",
    "base-task": "^3.0.0",
    "clone-deep": "^4.0.1",
    "data-store": "^3.1.0",
    "enquirer": "^2.3.0",
    "find-pkg": "^2.0.0",
    "get-value": "^3.0.1",
    "global-modules": "^2.0.0",
    "inflection": "^1.12.0",
    "kind-of": "^6.0.2",
    "log-utils": "^1.0.0",
    "matched": "^4.0.0",
    "merge-configs": "^3.0.0",
    "merge-deep": "^3.0.2",
    "omit-empty": "^1.0.0",
    "path-starts-with": "^2.0.0",
    "picomatch": "^2.0.6",
    "set-value": "^3.0.0",
    "strip-comments": "file:../../comments/strip-comments",
    "text-table": "^0.2.0",
    "union-value": "^2.0.0",
    "validate-npm-package-name": "^3.0.0",
    "vinyl": "^2.2.0",
    "word-wrap": "^1.2.3",
    "write": "^1.0.3",
    "yargs": "^13.2.2"
  },
  "devDependencies": {
    "assemble": "^0.24.3",
    "gulp-format-md": "^2.0.0",
    "mocha": "^6.1.4"
  },
  "keywords": [
    "dep",
    "dependencies",
    "dependency",
    "deps",
    "detect",
    "detective",
    "devDependencies",
    "extra",
    "find",
    "installed",
    "lint",
    "missing",
    "new",
    "require",
    "required",
    "requires",
    "unused"
  ],
  "eslintConfig": {
    "rules": {
      "no-console": 0
    }
  },
  "verb": {
    "toc": "collapsible",
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    }
  }
}