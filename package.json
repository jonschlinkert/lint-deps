{
  "name": "lint-deps",
  "description": "CLI tool that tells you when dependencies are missing from package.json and offers you a choice to install them. Also tells you when dependencies are listed in package.json but are not being used anywhere in your project. Node.js command line tool and API.",
  "version": "2.2.4",
  "main": "index.js",
  "homepage": "https://github.com/jonschlinkert/lint-deps",
  "author": {
    "name": "Jon Schlinkert",
    "url": "https://github.com/jonschlinkert"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jonschlinkert/lint-deps.git"
  },
  "bugs": {
    "url": "https://github.com/jonschlinkert/lint-deps/issues"
  },
  "license": {
    "type": "MIT",
    "url": "https://github.com/jonschlinkert/lint-deps/blob/master/LICENSE"
  },
  "files": ["index.js", "cli.js", "lib/"],
  "preferGlobal": true,
  "bin": {
    "deps": "cli.js"
  },
  "dependencies": {
    "arr-flatten": "^1.0.1",
    "array-unique": "^0.2.1",
    "chalk": "^1.0.0",
    "commandments": "^0.2.2",
    "debug": "^2.2.0",
    "filter-object": "^1.1.2",
    "get-value": "^1.1.4",
    "inquirer": "^0.8.3",
    "load-pkg": "^1.2.0",
    "lodash": "^3.8.0",
    "log-symbols": "^1.0.2",
    "look-up": "^0.7.1",
    "match-requires": "^1.0.0",
    "micromatch": "^2.1.6",
    "minimist": "^1.1.1",
    "parse-gitignore": "^0.1.2",
    "regexp-special-chars": "^0.1.0",
    "relative": "^3.0.0",
    "spawn-commands": "^0.2.0",
    "verbalize": "^0.1.2",
    "word-wrap": "^1.0.3",
    "write-json": "^0.2.0"
  },
  "keywords": [
    "dep",
    "dependencies",
    "dependency",
    "deps",
    "detect",
    "detective",
    "find",
    "installed",
    "lint",
    "missing",
    "require",
    "required",
    "requires"
  ]
}
