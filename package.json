{
  "name": "lint-deps",
  "description": "Lint deps",
  "version": "0.1.0",
  "homepage": "https://github.com/jonschlinkert/lint-deps",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "contributors": [
    "Brian Woodward (https://twitter.com/doowb)",
    "hemanth.hm (http://h3manth.com)",
    "Jake Verbaten (http://github.com/Raynos)",
    "Jon Schlinkert (http://twitter.com/jonschlinkert)",
    "Mark Dalgleish (markdalgleish.com)"
  ],
  "repository": "jonschlinkert/lint-deps",
  "bugs": {
    "url": "https://github.com/jonschlinkert/lint-deps/issues"
  },
  "license": "MIT",
  "files": [
    "bin",
    "index.js",
    "lib"
  ],
  "main": "index.js",
  "bin": {
    "deps": "bin/cli.js"
  },
  "engines": {
    "node": ">=4.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "arr-diff": "^4.0.0",
    "arr-union": "^3.1.0",
    "arrayify-compact": "^0.2.0",
    "async-each-series": "^1.1.0",
    "base": "^0.13.0",
    "base-cwd": "^0.3.4",
    "base-npm": "^0.4.1",
    "base-option": "^0.8.4",
    "base-pkg": "^0.2.4",
    "base-task": "^0.7.1",
    "base-yarn": "^1.0.0",
    "clone-deep": "^0.3.0",
    "data-store": "^1.0.0",
    "enquirer": "^1.0.2",
    "find-pkg": "^1.0.0",
    "fs-exists-sync": "^0.1.0",
    "get-value": "^2.0.6",
    "global-modules": "^0.2.3",
    "is-glob": "^3.1.0",
    "is-valid-glob": "^0.3.0",
    "kind-of": "^4.0.0",
    "koalas": "^1.0.2",
    "lazy-cache": "^2.0.2",
    "log-utils": "^0.2.1",
    "match-requires": "^1.0.2",
    "matched": "^0.4.4",
    "merge-deep": "^3.0.0",
    "mixin-deep": "^1.2.0",
    "object.omit": "^2.0.1",
    "object.pick": "^1.2.0",
    "omit-empty": "^0.4.1",
    "prompt-checkbox": "^2.0.1",
    "prompt-confirm": "^1.0.2",
    "prompt-radio": "^1.0.0",
    "set-value": "^1.0.0",
    "strip-comments": "^0.4.4",
    "text-table": "^0.2.0",
    "union-value": "^1.0.0",
    "validate-npm-package-name": "^3.0.0",
    "vinyl": "^2.0.2",
    "word-wrap": "^1.2.2",
    "write-json": "^1.0.1",
    "yargs": "^8.0.1"
  },
  "keywords": [
    "dependencies",
    "deps",
    "devDependencies",
    "extra",
    "lint",
    "missing",
    "new",
    "requires",
    "unused"
  ],
  "lintDeps": {
    "modules": {
      "exclude": [
        "merge-configs",
        "micromatch"
      ]
    }
  },
  "verb": {
    "toc": "collapsible",
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    },
    "related": {
      "list": []
    },
    "reflinks": []
  }
}
