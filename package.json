{
  "name": "lint-deps",
  "description": "CLI tool that tells you when dependencies are missing from package.json and offers you a choice to install them. Also tells you when dependencies are listed in package.json but are not being used anywhere in your project. Node.js command line tool and API.",
  "version": "2.5.2",
  "homepage": "https://github.com/jonschlinkert/lint-deps",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/lint-deps",
  "bugs": {
    "url": "https://github.com/jonschlinkert/lint-deps/issues"
  },
  "license": "MIT",
  "files": [
    "cli.js",
    "index.js",
    "lib/"
  ],
  "main": "index.js",
  "scripts": {
    "test": "mocha"
  },
  "preferGlobal": true,
  "bin": {
    "deps": "cli.js"
  },
  "dependencies": {
    "base": "^0.6.3",
    "base-cli": "^0.4.0",
    "base-config": "^0.3.3",
    "base-plugins": "^0.4.1",
    "component-emitter": "^1.2.0",
    "extend-shallow": "^2.0.1",
    "get-value": "^2.0.2",
    "has-glob": "^0.1.1",
    "lazy-cache": "^1.0.3",
    "match-requires": "^1.0.2",
    "matched": "^0.4.1",
    "micromatch": "^2.3.7",
    "minimist": "^1.2.0",
    "object.omit": "^2.0.0",
    "question-cache": "^0.3.5",
    "strip-comments": "^0.4.3",
    "to-file": "^0.1.5",
    "union-value": "^0.2.1"
  },
  "devDependencies": {
    "mocha": "*",
    "should": "*"
  },
  "keywords": [
    "dep",
    "dependencies",
    "dependency",
    "deps",
    "detect",
    "detective",
    "find",
    "installed",
    "lint",
    "missing",
    "require",
    "required",
    "requires"
  ],
  "verb": {
    "related": {
      "list": [
        "lookup-deps",
        "list-deps",
        "spawn-commands",
        "resolve-dep"
      ]
    }
  }
}
